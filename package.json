{
  "name": "frances-kornbluth",
  "description": "Artist website for Francis Kornbluth",
  "version": "2.0.0",
  "private": true,
  "author": "colinrobertbrooks@gmail.com",
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "prettier": "prettier \"{src,__{tests,mocks}__}/**/*.+(ts|tsx|json|css)\"",
    "format:check": "yarn prettier -- --list-different",
    "format:fix": "yarn prettier -- --write",
    "test:watch": "react-scripts test",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "validate": "npm-run-all lint format:check test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@types/react-gtm-module": "^2.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "focus-trap-react": "^8.3.2",
    "focus-visible": "^5.2.0",
    "polished": "^4.0.5",
    "query-string": "^6.13.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-router-dom": "^5.2.0",
    "react-select": "^3.1.1",
    "reactstrap": "^8.7.1",
    "styled-components": "^5.2.1",
    "use-query-params": "^1.1.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-select": "^3.0.28",
    "@types/reactstrap": "^8.7.2",
    "@types/styled-components": "^5.1.4",
    "cross-env": "^7.0.2",
    "eslint-config-colinrcummings": "^4.1.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://franceskornbluth.com"
}
